{
    "name": "one",
    "version": "0.0.1",
    "description": "A simple template for nodejs module",
    "main": "one.js",
    "private": "true",
    "author": "Paolo Manca",
    "scripts": {
        "test": "tape test.js | faucet",
        "lint": "standard",
        "sanity": "npm test && npm run lint",
        "profile": "node --prof",
        "postprofile": "node --prof-process isolate-0x*-v8.log > processed.log",
        "preprofile": "rm -f processed.txt && rm -f isolate-0x*-v8.log"
    },
    "keywords": [
        "template",
        "automation"
    ],
    "precommit": [
        "lint",
        "test"
    ],
    "license": "ISC",
    "dependencies": {
        "debug": "^4.1.0"
    },
    "devDependencies": {
        "faucet": "0.0.1",
        "tape": "^4.0.1",
        "tap-spec": "^5.0.0",
        "tape-catch": "^1.0.4",
        "pre-commit": "^1.0.10",
        "standard": "^12.0.1"
    }
}
